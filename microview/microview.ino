void displayTemperature(String val){
	if(val == "01d"){
		displayClearD();
	}else if(val == "01n"){
		displayClearD();
	}else if(val == "02d" || val == "04d"){
		displayCloud();
	}else if(val == "02n" || val == "04n"){
		displayCloud();
	}else if(val == "03d" || val == "03n"){
		displayCloud();
	}else if(val == "09d" || val == "09n" || val == "10d" || val == "10n"){
		displayRain();
	}else if(val == "11d" || val == "11n"){
		displayRain();
	}else if(val == "13d" || val == "13n"){
		displaySnow();
	}else if(val == "50d"){
		displayFogD();
	}else if(val == "50n"){
		displayFogD();
	}else{
		displayClearD();
	}
	
}

